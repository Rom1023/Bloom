<div class="container">
  <div class="my-5">
    <h2>Edit Project: <%= @project.name %></h2>

    <div class="w-50">
      <%= simple_form_for(@project) do |f| %>
        <%= f.input :name %>
        <%= f.input :description %>
        <%= f.input :completed %>
        <%= f.button :submit, class: "btn-s-blue" %>
      <% end %>
    </div>
  </div>
</div>
