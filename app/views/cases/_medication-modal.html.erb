
<!-- Button trigger modal -->
<button type="button" data-toggle="modal" data-target="#myModal<%= item.id %>">
  <div class="file-thumbnail" style="background-image: url('<%= cl_image_path item.key%>')"> </div>
</button>

<!-- Modal -->
<div class="modal fade" id="myModal<%= item.id %>" tabindex="-1" role="dialog" aria-labelledby="exampleModalLongTitle" aria-hidden="true">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="exampleModalLongTitle">Modal title</h5>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body">
        <div class="file-thumbnail" style="background-image: url('<%= cl_image_path item.key%>')"> </div>
      </div>
<!--  <div class="modal-footer">
        <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
      </div> -->
    </div>
  </div>
</div>
